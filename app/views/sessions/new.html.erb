<h1>Login Here:</h1>
<%= form_tag({controller: 'sessions', action: 'create', method: 'post'}) do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Submit "%>
<% end %>
<p>
  <%= link_to "Sign up here", new_user_path %>
</p>
<p>
  <%= link_to "...or login using Google!", '/auth/google_oauth2' %>
</p>